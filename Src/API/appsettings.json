    "PublicKeyPem": "-----BEGIN PUBLIC KEY-----\n...paste RS256 public key PEM...\n-----END PUBLIC KEY-----\n",
    "PrivateKeyPem": "-----BEGIN PRIVATE KEY-----\n...dev only, do not commit...\n-----END PRIVATE KEY-----\n",
    "AccessTokenMinutes": 30
  },
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:7050",
      "http://localhost:7050"
    ]
  },
  "Serilog": {
